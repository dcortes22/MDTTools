<%= simple_form_for(@section) do |f| %>
  <%= f.error_notification %>
  <%= f.input :order %>
  <%= f.association :plataforms, as: :check_boxes %>
  <%= f.input :name %>
  <%= f.input :media_url %>
  <%= f.input :config %>
  <%= f.association :sample %>
  <%= f.association :component %>
  <%= f.association :type %>
  <%= f.button :submit %>
<% end %>
